<!doctype html>
<html>

<head>
	<meta name="ac:route" content="/leasing">
	<base href="/">
	<script src="dmxAppConnect/dmxAppConnect.js"></script>
	<meta charset="UTF-8">
	<title>Leasing</title>
	<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous" />
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<link rel="stylesheet" href="bootstrap/4/css/bootstrap.min.css" />
	<script src="dmxAppConnect/dmxBootstrap4Navigation/dmxBootstrap4Navigation.js" defer=""></script>
	<script src="dmxAppConnect/dmxBrowser/dmxBrowser.js" defer=""></script>
	<script src="dmxAppConnect/dmxStateManagement/dmxStateManagement.js" defer=""></script>
	<script src="dmxAppConnect/dmxFormatter/dmxFormatter.js" defer=""></script>
</head>

<body is="dmx-app" id="leasing">
	<dmx-session-manager id="session"></dmx-session-manager>
	<div is="dmx-browser" id="redirect"></div>

	<div class="row">
		<div class="col text-center pb-1 border-bottom">
			<img src="assets/img/koenigsegg-agera-symbol.jpg" width="100" height="61">
		</div>
	</div>
	<nav class="navbar navbar-expand-lg navbar-light">
		<button id="models" class="btn flex-grow-1 btn-sm"><b>Models&nbsp;<i class="fa fa-car"></i></b></button>
		<button id="leasing" class="btn flex-grow-1 btn-sm"><b>Leasing</b></button>
		<button id="service" class="btn flex-grow-1 btn-sm"><b>Service&nbsp;<i class="fa fa-wrench"></i></b></button>
		<button id="login" class="btn flex-grow-1 btn-sm" dmx-hide="session.data.on.contains('on')"><b>Log in&nbsp;<i class="fa fa-sign-in"></i></b></button>
		<button id="account" class="btn flex-grow-1 btn-sm" dmx-show="session.data.on.contains('on')"><b>My Account&nbsp;<i class="fa fa-user"></i></b></button>
		<button id="logout" class="btn flex-grow-1 btn-sm" dmx-show="session.data.on.contains('on')"><b>Log out&nbsp;<i class="fa fa-sign-out"></i></b></button>
	</nav>
	<script src="bootstrap/4/js/popper.min.js"></script>
	<script src="bootstrap/4/js/bootstrap.min.js"></script>
</body>

</html>